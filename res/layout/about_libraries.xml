<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/scrollView1"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent" >
	<RelativeLayout
		android:id="@+id/RelativeLayout1"
		android:layout_width="fill_parent"
		android:layout_height="552dp"
		android:orientation="vertical" >
		<TextView
			android:id="@+id/textView1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_centerHorizontal="true"
			android:text="Libraries"
			android:textColor="@color/holo_blue"
			android:textAppearance="?android:attr/textAppearanceLarge" />
		<TextView
			android:id="@+id/textView2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView1"
			android:layout_centerHorizontal="true"
			android:text="CardsLib: Gabriele Mariotti"
			android:textAppearance="?android:attr/textAppearanceMedium" />
		<TextView
			android:id="@+id/textView3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView2"
			android:layout_centerHorizontal="true"
			android:text="Changelog Viewer: Gabriele Mariotti"
			android:textAppearance="?android:attr/textAppearanceMedium" />
		<TextView
			android:id="@+id/textView4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView3"
			android:layout_centerHorizontal="true"
			android:text="Sliding Menu: Jeremy Feinstein"
			android:textAppearance="?android:attr/textAppearanceMedium" />
		<TextView
			android:id="@+id/textView5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView4"
			android:layout_centerHorizontal="true"
			android:text="Updater: CommonsWare"
			android:textAppearance="?android:attr/textAppearanceMedium" />
		<TextView
			android:id="@+id/textView6"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView5"
			android:layout_centerHorizontal="true"
			android:text="App Rater: CodeChimp"
			android:textAppearance="?android:attr/textAppearanceMedium" />
		<TextView
			android:id="@+id/textView7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView6"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="48dp"
			android:autoLink="web"
			android:text="https://github.com/gabrielemariotti/cardslib"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		<TextView
			android:id="@+id/textView8"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/textView7"
			android:layout_centerHorizontal="true"
			android:autoLink="web"
			android:text="https://github.com/gabrielemariotti/changeloglib"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		<TextView
			android:id="@+id/textView9"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignLeft="@+id/textView3"
			android:layout_below="@+id/textView8"
			android:autoLink="web"
			android:text="https://github.com/jfeinstein10/SlidingMenu"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		<TextView
			android:id="@+id/textView10"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignRight="@+id/textView4"
			android:layout_below="@+id/textView9"
			android:autoLink="web"
			android:text="https://github.com/rgocal/cwac-updater"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		<TextView
			android:id="@+id/textView11"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignRight="@+id/textView9"
			android:layout_below="@+id/textView10"
			android:autoLink="web"
			android:text="https://github.com/codechimp-org/AppRater"
			android:textAppearance="?android:attr/textAppearanceSmall" />
	</RelativeLayout>
</ScrollView>